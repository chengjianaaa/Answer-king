page {
  height: 100%;
}

.container {
  display: block;
  height: 100%;
}

.practice_list {
  color: #fff;
  margin: 0 30rpx;
  padding: 45rpx 40rpx;
  background: linear-gradient(to right, #3b6bc8, #11156d);
}

.list_left {
  display: inline-block;
  width: 50%;
  font-size:40rpx;
  text-align: center;
  vertical-align: top;
}
.list_left navigator{
  width:180rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  margin:30rpx auto 0;
  border: 1px solid #fff;
  border-radius: 50px;
}
.list_right {
  display: inline-block;
  width: 50%;
  text-align: center;
}
.list_right text{
  display: inline-block;
  font-size: 25rpx;
  margin: 20rpx 0;
}
.list_right .iz{
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  margin: 0 20rpx;
}
.modlist-box{
  margin: 10rpx 30rpx 0;
  padding-bottom:100rpx;
  border-radius: 10rpx;
  overflow: hidden;
  background: #fff;
}
.modlist{
  width: 100%;
  height: 180rpx;
  padding:40rpx 30rpx;
  border-bottom: 1px dotted #ccc;
  box-sizing: border-box;
  background: #fff;
}
.modli-title{
  display: inline-block;
}
.modli-title .modli-idx{
  display: inline-flex;
  width: 50rpx;
  height: 50rpx;
  justify-content:center;
  align-items: center;
  font-size: 30rpx;
  color: #fff;
  margin-right: 20rpx;
  border-radius: 50%;
  background: #f79940;
}
.modlist .progress{
  display: inline-block;
  width: 360rpx;
  margin-top:30rpx;
  margin-left:70rpx; 
  border-radius:50px;
  overflow:hidden;
}
.pnumder{
  margin-left:30rpx;
  vertical-align: middle;
}
.ih-qbi{
  position: absolute;
  color: #cccc;
  right:66rpx;
  font-size: 50rpx;
}





