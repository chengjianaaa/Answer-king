/**app.wxss**/
.container {
  min-height: 100%;
  display: flex;
  box-sizing: border-box;
} 
/*底部导航*/
.nav{
  display: flex;
  position:fixed;
  bottom: 0;
  background: #150c33;
  width: 100%;
  justify-content:center;
}
.nav .iz{
  font-size: 44rpx;
}
.nav navigator{
  display: flex;
  width: 25%;
  font-size: 20rpx;
  color: #888888;
  flex-direction:column;
  text-align: center;
  justify-content: center;
  height:100rpx;
}
.nav .active{
  color: #fff;
}
