/* pages/battles/battles.wxss */
@import "../../utils/animate.wxss";

page {
  background: #070432;
}

.container {
  padding: 0 10rpx;
}

.pk-title {
  display: flex;
  justify-content: center;
}

.userinfo-nickname {
  width: 140rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 60rpx;
}

.userinfo-avatar {
  display: block;
  width: 85rpx;
  height: 85rpx;
  margin: 15rpx 15rpx;
  border-radius: 100px;
  overflow: hidden;
}

.mycontent {
  position: relative;
  width: 260rpx;
  height: 150rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
  opacity: 0;
}

.mycontent .userinfo-nickname {
  position: absolute;
  top: 0;
  left: 150rpx;
}

.mycontent .myavater-box {
  position: absolute;
  top: 0;
  left: 0;
  width: 150rpx;
  height: 150rpx;
  box-sizing: border-box;
  border: 20rpx solid rgba(45, 99, 190, 0.2);
  border-radius: 50%;
  background: url('data:image/png;base64,')no-repeat;
  background-size: 100% 100%;
}

.myintegral {
  position: absolute;
  width: 170rpx;
  height: 65rpx;
  line-height: 65rpx;
  top: 60rpx;
  left: 95rpx;
  padding-left: 10rpx;
  color: #fff;
  text-align: center;
  font-size: 45rpx;
  font-weight: bold;
  background: -webkit-linear-gradient(left, #3b5abb, #060331);
  background: -o-linear-gradient(left, #3b5abb, #060331);
  background: -moz-linear-gradient(left, #3b5abb, #060331);
  background: linear-gradient(to right, #3b5abb, #060331);
}

.itcontent {
  position: relative;
  width: 260rpx;
  height: 150rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-right: 260rpx;
  opacity: 0;
}

.itcontent .userinfo-nickname {
  position: absolute;
  top: 0;
  right: 160rpx;
  text-align: right;
}

.itcontent .itavater-box {
  position: absolute;
  top: 0;
  right: 0;
  width: 150rpx;
  height: 150rpx;
  box-sizing: border-box;
  border: 20rpx solid rgba(127, 93, 56, 0.2);
  border-radius: 50%;
  background: url('data:image/png;base64,')no-repeat;
  background-size: 100% 100%;
}

.itintegral {
  position: absolute;
  width: 170rpx;
  height: 65rpx;
  line-height: 65rpx;
  top: 60rpx;
  right: 95rpx;
  padding-left: 10rpx;
  color: #fff;
  text-align: center;
  font-size: 45rpx;
  font-weight: bold;
  background: -webkit-linear-gradient(left, #0a0938, #e1991e);
  /* Opera 11.1 - 12.0 */
  background: -o-linear-gradient(left, #0a0938, #e1991e);
  /* Firefox 3.6 - 15 */
  background: -moz-linear-gradient(left, #0a0938, #e1991e);
  /* 标准的语法 */
  background: linear-gradient(to right, #0a0938, #e1991e);
}

.countdown {
  display: flex;
  width: 150rpx;
  height: 150rpx;
  margin: 20rpx 20rpx;
  color: #fff;
  justify-content: center;
  align-items: center;
}

.itcontent {
  width: 260rpx;
  height: 150rpx;
  margin: 20rpx 20rpx;
}

/* .pk-bottombox{
position: relative;
  height: 200rpx;
  color: #fff;
} */

.pk-bottom image, .pk-bottom text {
  position: absolute;
  display: inline-block;
  width: 350rpx;
  height: 130rpx;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
}

.pk-bottom text {
  padding-top: 40rpx;
  box-sizing: border-box;
}

.pk-bottom {
  position: relative;
  color: #fff;
  text-align: center;
  top: 20rpx;
  opacity: 0;
}

.subjectbox {
  /* position:absolute; */
  margin-top: 70rpx;
  opacity: 0;
}

/*题目*/

.subject {
  display: inline-block;
  color: #fff;
  font-size: 32rpx;
  line-height: 50rpx;
  padding: 0 70rpx;
  text-align: top;
}

.subject-label {
  font-size: 28rpx;
  color: #7093f6;
  display: block;
}

.option {
  padding: 40rpx 40rpx;
}

.option button {
  display: flex;
  min-width: 500rpx;
  justify-content: space-between;
  align-items: center;
  min-height: 80rpx;
  margin-top: 20rpx;
  padding: 10rpx 0;
  border-radius: 10px;
}

.option button text {
  min-width: 30rpx;
  line-height: 50rpx;
  text-align: left;
  white-space: normal;
  word-wrap: break-word;
}

.option button .iz {
  color: #f8f8f8;
  font-size: 40rpx;
  width: 40rpx;
  margin: 0 30rpx;
}

.button-hover {
  background-color: red;
}

/*答题结束页*/

.victory, .fail {
  position: relative;
}

.victory text, .fail text {
  position: absolute;
  display: inline-block;
  width: 280rpx;
  height: 180rpx;
  line-height: 170rpx;
  color: #fff;
  font-weight: bolder;
}

.meitintegral {
  padding-left: 20rpx;
  left: -410rpx;
  border-top-right-radius: 50px;
  border-bottom-right-radius: 50px;
  background: -webkit-linear-gradient(left, #060331, #3b5abb);
  background: -o-linear-gradient(left, #060331, #3b5abb);
  background: -moz-linear-gradient(left, #060331, #3b5abb);
  background: linear-gradient(to right, #060331, #3b5abb);
}

.victory .meitintegral {
  padding-right: 100rpx;
  font-size: 84rpx;
}

.heiitintegral {
  right: -410rpx;
  padding-right: 20rpx;
  text-align: right;
  border-top-left-radius: 50px;
  border-bottom-left-radius: 50px;
  background: -webkit-linear-gradient(left, #e1991e, #0a0938);
  background: -o-linear-gradient(left, #e1991e, #0a0938);
  background: -moz-linear-gradient(left, #e1991e, #0a0938);
  background: linear-gradient(to right, #e1991e, #0a0938);
}

.victory .heiitintegral {
  font-size: 60rpx;
}

.victory-img {
  position: absolute;
  top: -220rpx;
  left: 0;
  right: 0;
  width: 100%;
  height: 360rpx;
  margin: auto;
}

.victory-bom {
  position: absolute;
  top: 300rpx;
  width: 100%;
}

.victory-continue, .victory-flaunt {
  width: 460rpx;
  height: 100rpx;
  margin-top: 20rpx;
  font-size: 40rpx;
  margin: 20rpx auto 0;
  border-radius: 10rpx;
  text-align: center;
  line-height: 100rpx;
}

.victory-review {
  height: 150rpx;
  width: 400rpx;
  margin: 0 auto 100rpx;
  background: url('data:image/png;base64,')no-repeat;
  background-size: 100% 100%;
}

.victory-continue {
  color: #2a4cac;
  background: #fff;
}

.victory-flaunt {
  color: #fff;
  background: #ed8910;
}

.victory-flaunt image {
  display: inline-block;
  width: 45rpx;
  height: 45rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}

.fail .meitintegral {
  font-size: 60rpx;
}

.fail .heiitintegral {
  font-size: 84rpx;
  padding-left: 100rpx;
}

.studys {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
  font-size: 40rpx;
}

.studys .victory-share {
  height: 100rpx;
  width: 280rpx;
  color: #fff;
  background: -webkit-linear-gradient(left, #ed8910, #ed9c10);
  background: -o-linear-gradient(left, #ed8910, #ed9c10);
  background: -moz-linear-gradient(left, #ed8910, #ed9c10);
  background: linear-gradient(to right, #ed8910, #ed9c10);
}

.studys .victory-studys {
  height: 100rpx;
  width: 280rpx;
  margin: 0 45rpx;
  color: #fff;
  line-height: 100rpx;
  text-align: center;
  border-radius: 10rpx;
  background: -webkit-linear-gradient(left, #2c83cd, #3459ca);
  background: -o-linear-gradient(left, #2c83cd, #3459ca);
  background: -moz-linear-gradient(left, #2c83cd, #3459ca);
  background: linear-gradient(to right, #2c83cd, #3459ca);
}

.studys image {
  display: inline-block;
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
