/* pages/analysis/analysis.wxss */
page {
  height: 100%;
}

.container {
  display: block;
  height: 100%;
  overflow: scroll;
}
swiper {
  height: 100%;
}
.an-title{
  width: 100%;
  height: 110rpx;
  padding: 0 20rpx;
  color: #f6aa14;
  font-weight:bold;
  line-height: 110rpx;
  background:rgba(62, 78, 167, .3);
  box-sizing: border-box;
}
.an-title image{
  width: 80rpx;
  height: 100rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.an-topic{
  width: 100%;
  padding:0 45rpx;
  font-size: 28rpx;
  box-sizing: border-box;
}
.an-box{
  line-height:50rpx;
  margin-top: 30rpx;
  padding: 0 20rpx 30rpx;
  border-radius: 5px;
  font-weight: bold;
  background: #fff;
}
.an-subject{
  display: inline-block;
  width: 100%;
  padding: 30rpx 40rpx 0;
  box-sizing: border-box;
}
.an-option{
  width: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.an-answer{
  display:  inline-block;
  width: 100%;
  line-height: 50rpx;
  margin:30rpx 0;
  padding:20rpx 30rpx;
  border-radius: 5px;
  font-weight: bold;
  background: #fff;
  box-sizing: border-box;
}
.an-answer text{
  display: block;
}